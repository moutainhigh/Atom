<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.pull.mapper.ApiGgpGameinfoMapper">
	<!--插入记录 -->
	<insert id="insert" parameterType="hashmap">
		replace into api_ggp_gameinfo(lsh,gamedate,username,ngr)  values (#{lsh},#{gamedate},#{username},#{ngr})
	</insert>

	<!--批量插入记录 -->
	<insert id="batchInsert" parameterType="list">
		replace into api_ggp_gameinfo(lsh,gamedate,username,ngr)  values
		<foreach item="item" index="index" collection="list" separator=",">
			(#{item.lsh},#{item.gamedate},#{item.username},#{item.ngr})
		</foreach>
	</insert>
	
	<!--选择插入记录 -->
	<insert id="optInsert" parameterType="hashmap">
		insert into api_ggp_gameinfo
		<trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="lsh != null">
				lsh,
			</if>
			<if test="gamedate != null">
				gamedate,
			</if>
			<if test="username != null">
				username,
			</if>
			<if test="ngr != null">
				ngr
			</if>
		</trim>
		<trim prefix=" values (" suffix=")" suffixOverrides="," >
			<if test="lsh != null">
				#{lsh},
			</if>
			<if test="gamedate != null">
				#{gamedate},
			</if>
			<if test="username != null">
				#{username},
			</if>
			<if test="ngr != null">
				#{ngr}
			</if>
		</trim>
	</insert>

	<!--根据实体删除记录 -->
	<delete id="delete" parameterType="hashmap">
		delete from api_ggp_gameinfo
		<where>
			<if test="lsh != null">
				lsh=#{lsh}
			</if>
			<if test="gamedate != null">
				and gamedate=#{gamedate}
			</if>
			<if test="username != null">
				and username=#{username}
			</if>
			<if test="ngr != null">
				and ngr=#{ngr}
			</if>
			<if test="ids != null">
				and lsh in (${ids})
			</if>

		</where>
	</delete>

	<!--更新数据操作 -->
	<update id="update" parameterType="hashmap">
		update api_ggp_gameinfo
		<set>
			<if test="gamedate != null">
				gamedate=#{gamedate},
			</if>
			<if test="username != null">
				username=#{username},
			</if>
			<if test="ngr != null">
				ngr=#{ngr}
			</if>
		</set>
		where lsh=#{lsh}
	</update>

	<!--批量更新记录 -->
	<update id="batchUpdate" parameterType="list">
		<foreach item="item" index="index" collection="array" separator=",">
			update api_ggp_gameinfo set
			gamedate=#{item.gamedate},username=#{item.username},ngr=#{item.ngr}
			where lsh =#{item.lsh}
		</foreach>
	</update>

	<!--根据主键查询单条记录 -->
	<select id="selectByPrimaryKey" resultType="hashmap" parameterType="string">
		select * from api_ggp_gameinfo a where a.lsh=#{key}
	</select>

	<!--查询�?��记录 -->
	<select id="selectAll" resultType="hashmap">
		select * from api_ggp_gameinfo a
	</select>

	<!--通过实体字段查询记录 -->
	<select id="selectByEntity" parameterType="hashmap" resultType="hashmap">
		select * from api_ggp_gameinfo a
		<where>
			<if test="lsh != null">
				a.lsh=#{lsh}
			</if>
			<if test="gamedate != null">
				and a.gamedate=#{gamedate}
			</if>
			<if test="username != null">
				and a.username=#{username}
			</if>
			<if test="ngr != null">
				and a.ngr=#{ngr}
			</if>

		</where>
		<choose>  
            <when test="order != null ">  
                order by a.${order}
            </when >             
            <otherwise>  
            	order by a.lsh asc
            </otherwise>  
        </choose>
	</select>

	<!--通过实体字段模糊查询记录 -->
	<select id="selectByEntityLike" parameterType="hashmap" resultType="hashmap">
		select * from api_ggp_gameinfo a
		<where>
			<if test="lsh != null">
				 a.lsh like '%${lsh}%'
			</if>
			<if test="gamedate != null">
				and a.gamedate like '%${gamedate}%'
			</if>
			<if test="username != null">
				and a.username like '%${username}%'
			</if>
			<if test="ngr != null">
				and a.ngr like '%${ngr}%'
			</if>

		</where>
		<choose>  
            <when test="order != null ">  
                order by a.${order}
            </when >             
            <otherwise>  
            	order by a.lsh asc
            </otherwise>  
        </choose>
	</select>

	<!--通过实体字段模糊查询记录分页 -->
	<select id="selectByEntityLikePage" parameterType="hashmap" resultType="hashmap">
		select * from api_ggp_gameinfo a
		<where>
			<if test="lsh != null">
				 a.lsh like '%${lsh}%'
			</if>
			<if test="gamedate != null">
				and a.gamedate like '%${gamedate}%'
			</if>
			<if test="username != null">
				and a.username like '%${username}%'
			</if>
			<if test="ngr != null">
				and a.ngr like '%${ngr}%'
			</if>

		</where>
		<choose>  
            <when test="order != null ">  
                order by a.${order}
            </when >             
            <otherwise>  
            	order by a.lsh asc
            </otherwise>  
        </choose>
		limit #{pageIndex},#{pageSize}
	</select>

	<!--通过实体字段�?��结果总数 -->
	<select id="count" resultType="int" parameterType="hashmap">
		select count(*) as total from api_ggp_gameinfo a
		<where>
			<if test="lsh != null">
				 a.lsh like '%${lsh}%'
			</if>
			<if test="gamedate != null">
				and a.gamedate like '%${gamedate}%'
			</if>
			<if test="username != null">
				and a.username like '%${username}%'
			</if>
			<if test="ngr != null">
				and a.ngr like '%${ngr}%'
			</if>

		</where>
	</select>

	<!--通过实体字段�?��结果按某个字段求和汇�?-->
	<select id="sum" resultType="string" parameterType="hashmap">
		select COALESCE(sum(${COLUMN}),0) from api_ggp_gameinfo a
		<where>
			<if test="lsh != null">
				 a.lsh like '%${lsh}%'
			</if>
			<if test="gamedate != null">
				and a.gamedate like '%${gamedate}%'
			</if>
			<if test="username != null">
				and a.username like '%${username}%'
			</if>
			<if test="ngr != null">
				and a.ngr like '%${ngr}%'
			</if>

		</where>
	</select>
</mapper>